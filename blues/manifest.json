{
    "name": "highballuos",
    "version": "1.0",
    "description": "Get Input's value When focus change!",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "options_page": "src/option/options.html",
    "content_scripts": [
      {
        "matches"  : ["https://*/*"],
        "js" : [
          "src/lib/lottie.min.js",
          "src/lib/tf.min.js",
          "src/js/util/constants.js",
          "src/js/util/event.js",
          "src/js/blues/BluesView.js",
          "src/js/blues/BluesModel.js",
          "src/js/blues/BluesPresenter.js",
          "src/js/index.js"
        ],
        "css" : ["src/css/blues.css"]
      }
    ],
    "web_accessible_resources": [
      "assets/images/eco_bulb.json",
      "assets/models/detect/model.json"
    ],
    "browser_action": {
      "default_popup": "src/popup/popup.html",
      "default_icon": {
        "32": "assets/images/eco_bulb_on32.png",
        "128": "assets/images/eco_bulb_on32.png"
      }
    },
    "icons": {
      "32": "assets/images/eco_bulb_on32.png",
      "128": "assets/images/eco_bulb_on32.png"
    },
    "manifest_version": 2
  }
  